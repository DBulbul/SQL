--Q.3.Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.
UPDATE EMPLOYEE
SET 
name='Dilara',
email='dilara@bulbul.com',
birthday='1995-10-06'
WHERE id=6
RETURNING *;

UPDATE EMPLOYEE
SET 
id=5,
email='dilara@bulbul.com',
birthday='1995-11-06'
WHERE name='Kevin'
RETURNING *;

UPDATE EMPLOYEE
SET 
id=63,
name='Mert',
birthday='1995-06-06'
WHERE email='ktamplinq@jiathis.com'
RETURNING *;

UPDATE EMPLOYEE
SET 
id=120,
name ='Mehmet',
email='m@bulbul.com'
WHERE birthday='2007-02-17'
RETURNING *;

UPDATE EMPLOYEE
SET 
name='Madeline',
email='d@bulbul.com',
birthday='1995-05-06'
WHERE id=42
RETURNING *;

--Q.4.Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.
DELETE FROM employee
WHERE id=5
RETURNING *;

DELETE FROM employee
WHERE name='Roderic'
RETURNING *;

DELETE FROM employee
WHERE email='anafzgery@cbslocal.com'
RETURNING *;

DELETE FROM employee
WHERE birthday='2011-06-04'
RETURNING *;

DELETE FROM employee
WHERE id>25
RETURNING *;
